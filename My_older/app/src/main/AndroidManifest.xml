<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.j.my_older">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResignActivity"
            android:label="@string/title_register"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name=".ResignActivity"
                android:value="com.example.j.my_older.MyActivity" />
        </activity>
        <activity android:name=".homeActivity" />
        <activity android:name=".ContactorsActivity" />
        <activity android:name=".QuitActivity" />
        <activity android:name=".ConfigureActivity" />
        <activity android:name=".HealthDetialsActivity" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".add_contact_activity"></activity>
    </application>

</manifest>